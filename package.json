{
  "name": "@elunic/ecs",
  "version": "0.1.3",
  "description": "The elunic coding styles",
  "files": [
    "commitlint/",
    "editorconfig/",
    "prettier/",
    "tsconfig/",
    "tslint/",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "check": "prettier --config ./prettier.config.js --check './**/*.js'",
    "fix": "prettier --config ./prettier.config.js --write --check './**/*.js'",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --config ./prettier.config.js --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elunic/ecs.git"
  },
  "keywords": [
    "elunic",
    "typescript",
    "tslint",
    "prettier",
    "commitlint",
    "styles",
    "ecs"
  ],
  "author": {
    "name": "William Hefter",
    "email": "wh@elunic.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "jasmine": "^3.3.1",
    "lint-staged": "^8.1.0"
  },
  "dependencies": {
    "prettier": "^1.16.1",
    "gts": "^0.9.0",
    "tslint-config-prettier": "^1.17.0"
  },
  "peerDependencies": {
    "@commitlint/config-conventional": "^7.5.0",
    "tslint": "^5.14.0",
    "typescript": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
